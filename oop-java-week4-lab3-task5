
package unilab;
 
 
public class tsk5 {
    public static void main(String[] args) 
    {
        int a=1;
        int b=5;
        for (int i=a; i<=b; i++) 
        {
            System.out.println("Multiplication Table for "+i+":");
            for (int j = 1; j <= 10; j++) 
            {
                System.out.println(i+" x "+j+" = "+(i*j));
            }
            System.out.println();
        }
    }
}
