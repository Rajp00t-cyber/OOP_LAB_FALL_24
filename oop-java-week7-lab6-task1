package unilab;
class Book
{
 private String title;
 private String author;
 private String isbn;
 // setters
 public void setTitle(String title) {
 this.title = title;
 }

 public void setAuthor(String author) {
 this.author = author;
 }
 public void setIsbn(String isbn) {
 this.isbn = isbn;
 }
 // getters
 public String getTitle() {
 return title;
 }
 public String getAuthor() {
 return author;
 }
 public String getIsbn() {
 return isbn;
 }

}
public class Encaps
{
 public static void main(String[] args)
 {
 Book b1 = new Book();
 b1.setAuthor("Rida");
 b1.setTitle("Journal");
 b1.setIsbn("abc");

 //cout
 System.out.println("Book Author is " + b1.getAuthor());
 System.out.println("Book title is " + b1.getTitle());
 System.out.println("Book isbn is " + b1.getIsbn());
 }
}
