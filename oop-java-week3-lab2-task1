
package unilab;

class Student {
   
    String name;
    String city;
    int age;
}
public class UniLab {
    public static void main(String[] args) {
        
        Student s1 = new Student();
        
        s1.name = "Amna";
        s1.city="Islamabad";
        s1.age=18;
        
        Student s2 = new Student();
        
        s2.name = "Sandy";
        s2.city="Bani gala";
        s2.age=22;
        
        
        
        System.out.println("Student's name is "+s1.name);
        System.out.println("Student's city is "+s1.city);
        System.out.println("Student's age is "+s1.age);
        
        System.out.println("\nStudent's name is "+s2.name);
        System.out.println("Student's city is "+s2.city);
        System.out.println("Student's age is "+s2.age);
    }
}
